<configuration>
    <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{dd MMM HH:mm:ss} %-5p %25.25c..%-10.10M:%-3L - %m%n</pattern>
        </encoder>
    </appender>

    <logger name="zhy2002" level="DEBUG">
        <!--<appender-ref ref="console"/>-->
    </logger>

    <root level="INFO">
        <appender-ref ref="console"/>
    </root>
</configuration>